{
  "log": { "level": "error" },
  "inbounds": [
    { "type": "mixed", "listen": "127.0.0.1", "listen_port": 1080 }
  ],
  "outbounds": [
    {
      "tag": "ws",
      "type": "vless",
      "server": "{{ proxy_masked_domain }}",
      "server_port": 443,
      "uuid": "{{ client_uuid }}",
      "tls": {
        "enabled": true,
        "server_name": "{{ proxy_masked_domain }}"
      },
      "transport": { "type": "ws", "path": "{{ client_ws_path }}" }
    },
    { "type": "direct", "tag": "direct" }
  ],
  "route": { "final": "ws" }
}
